<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="chatting">
	<insert id="insertChatting" parameterType="chatting">
		INSERT INTO CHATTING VALUES(SEQ_CHATNO.NEXTVAL,#{chatType},#{sender},#{receiver},DEFAULT,#{chatting},#{roomNo})
	</insert>
	
	<select id="selectChatting" resultType="chatting" parameterType="_int">
		SELECT * FROM CHATTING WHERE SENDER = #{userNo} OR RECEIVER = #{UserNo}
	</select>
</mapper>
